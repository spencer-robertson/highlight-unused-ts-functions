{
  "name": "highlight-unused-typescript-functions",
  "displayName": "Highlight unused Typescript functions",
  "description": "Highlight exported Typescript functions that are not being referenced in other files.",
  "version": "0.0.1",
  "author": "Spencer Robertson",
  "license": "MIT",
  "repository": "https://github.com/spencer-robertson/highlight-unused-exported-functions",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#86836D",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "publisher": "SpencerRobertson",
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
